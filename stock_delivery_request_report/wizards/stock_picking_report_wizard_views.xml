<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_picking_report_wizard" model="ir.ui.view">
        <field name="name">stock.picking.report.wizard</field>
        <field name="model">stock.picking.report.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="partner_id" options="{'no_create_edit': True, 'no_open': True, 'no_create': True}"/>
                    <field name="user_id" options="{'no_create_edit': True, 'no_open': True, 'no_create': True}"/>
                    <field name="report_date"/>
                </group>
                <footer>
                    <button name="print_delivery_request_form" type="object" string="Export Requst Form" class="oe_highlight"/>
                        or
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
